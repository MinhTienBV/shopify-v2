<header id="header" class="module header fixed z-50 w-full bg-gray-50 py-10 lg:py-0" data-module="header">
  <a
    class="skip-link text-center opacity-0 absolute hidden mx-auto w-full bg-black text-white"
    href="javascript:;"
    ><span>Skip to content</span></a
  >
  <nav class="container navbar navbar-expand-lg lg:flex mx-auto items-center justify-between">
    <div
      class="header-mobile row items-center justify-between flex-wrap"
    >
      <div class="col col-9 col-lg-12">
        <a
          id="header-logo"
          class="navbar-brand header-logo w-full"
          href="{{ routes.root_url }}"
        >
         {%- if section.settings.logo != blank -%}
          {%- assign logo_alt = section.settings.logo.alt | default: shop.name | escape -%}
          {{ section.settings.logo | image_url: width: 180 | image_tag:
                class: 'header__heading-logo',
                widths: '50, 100, 150, 200, 250, 300, 400, 500',
                width: 118,
                alt: logo_alt
            }}
          {%- else -%}
            <img src="//cdn.agilitycms.com/9th-wonder/Attachments/9thwonder%20assets/logo-R-home.svg" alt="TailWind">
          {%- endif -%}
        </a>
      </div>
      <div class="col col-3 text-right lg:hidden">
        <button
          class="border-0 hamburger-menu"
          type="button"
          data-toggle="collapse"
          data-target="#main-menu"
          aria-controls="main-menu"
          aria-expanded="false"
          aria-label="Toggle navigation"
        >
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icomoon icon-close hidden"></span>
          <span class="sr-only">Open Menu</span>
        </button>
      </div>
    </div>
    <div class="wrap-all-menu flex flex-wrap">
      <div class="navbar-collapse main-menu lg:relative lg:z-2 lg:mb-10 lg:w-1/2" id="main-top" data-module="menu">
        <ul class="main-menu-ul navbar-nav ml-auto lg:flex list-none pl-0 mb-0 pt-10 lg:pt-0">
        {%- for link in section.settings.menu_top.links -%}
            <li
              class="menu-item has-sub px-5 relative xl:px-10 pb-7 group mb-0 lg:py-10 {% if link.current %} current-menu-ancestor active {% endif %} {% if link.child_active %} current-menu-ancestor active {% endif %}">
              <a href="{{ link.url }}">{{ link.title }}</a>
              {%- if link.links != blank -%}
               <div class="nav-item-arrows inline-block arrows-lv1 absolute right-5 top-3 lg:hidden cursor-pointer">
              <i class="icomoon icon-chevron-down" aria-hidden="true"></i>
            </div>
            <div class="dropdown-menu rounded-0 border-0 lg:absolute hidden top-full lg:bg-white p-8 lg:group-hover:block">
              <ul class="list-inline list-none pl-0">
              {%- for childlink in link.links -%}
              <li
                  class="menu-item menu-item-type-taxonomy menu-item-object-product_cat menu-item-has-children {% if childlink.current %} current-menu-item active {% endif %} {% if childlink.child_active %} current-menu-ancestor active {% endif %}">
                  <a href="{{ childlink.url }}">{{ childlink.title }}
                  </a>
                  {%- if childlink.links != blank -%}
                  <ul class="sub-menu">
                    {%- for childlinkLV2 in childlink.links -%}
                    <li
                      class="menu-item menu-item-type-post_type menu-item-object-product {% if childlinkLV2.current %} current-menu-item active {% endif %}"><a
                        href="{{ childlinkLV2.url }}">{{ childlinkLV2.title }}</a>
                    </li>
                    {%- endfor -%}
                  </ul>
                  {%- endif -%}
                </li>
              {%- endfor -%}
              </ul>
              </div>
               {%- endif -%}
            </li>
            {%- endfor -%}
        </ul>
      </div>
      <div class="wrap-cart lg:mb-10 lg:w-1/2">
          
      </div>
      <div class="navbar-collapse main-menu lg:relative lg:z-1 lg:w-1/2" id="main-menu" data-module="menu">
        <ul class="main-menu-ul navbar-nav ml-auto lg:flex list-none pl-0 mb-0 pt-10 lg:pt-0">
        {%- for link in section.settings.menu.links -%}
            <li
              class="menu-item has-sub px-5 relative xl:px-10 pb-7 group mb-0 lg:py-10 {% if link.current %} current-menu-ancestor active {% endif %} {% if link.child_active %} current-menu-ancestor active {% endif %}">
              <a href="{{ link.url }}">{{ link.title }}</a>
              {%- if link.links != blank -%}
               <div class="nav-item-arrows inline-block arrows-lv1 absolute right-5 top-3 lg:hidden cursor-pointer">
              <i class="icomoon icon-chevron-down" aria-hidden="true"></i>
            </div>
            <div class="dropdown-menu rounded-0 border-0 lg:absolute hidden top-full lg:bg-white p-8 lg:group-hover:block">
              <ul class="list-inline list-none pl-0">
              {%- for childlink in link.links -%}
              <li
                  class="menu-item menu-item-type-taxonomy menu-item-object-product_cat menu-item-has-children {% if childlink.current %} current-menu-item active {% endif %} {% if childlink.child_active %} current-menu-ancestor active {% endif %}">
                  <a href="{{ childlink.url }}">{{ childlink.title }}
                  </a>
                  {%- if childlink.links != blank -%}
                  <ul class="sub-menu">
                    {%- for childlinkLV2 in childlink.links -%}
                    <li
                      class="menu-item menu-item-type-post_type menu-item-object-product {% if childlinkLV2.current %} current-menu-item active {% endif %}"><a
                        href="{{ childlinkLV2.url }}">{{ childlinkLV2.title }}</a>
                    </li>
                    {%- endfor -%}
                  </ul>
                  {%- endif -%}
                </li>
              {%- endfor -%}
              </ul>
              </div>
               {%- endif -%}
            </li>
            {%- endfor -%}
        </ul>
      </div>
      <div class="navbar-collapse main-menu lg:relative lg:z-1 lg:w-1/2" id="main-right" data-module="menu">
        <ul class="main-menu-ul navbar-nav ml-auto lg:flex list-none pl-0 mb-0 pt-10 lg:pt-0">
        {%- for link in section.settings.menu_right.links -%}
            <li
              class="menu-item has-sub px-5 relative xl:px-10 pb-7 group mb-0 lg:py-10 {% if link.current %} current-menu-ancestor active {% endif %} {% if link.child_active %} current-menu-ancestor active {% endif %}">
              <a href="{{ link.url }}">{{ link.title }}</a>
              {%- if link.links != blank -%}
               <div class="nav-item-arrows inline-block arrows-lv1 absolute right-5 top-3 lg:hidden cursor-pointer">
              <i class="icomoon icon-chevron-down" aria-hidden="true"></i>
            </div>
            <div class="dropdown-menu rounded-0 border-0 lg:absolute hidden top-full lg:bg-white p-8 lg:group-hover:block">
              <ul class="list-inline list-none pl-0">
              {%- for childlink in link.links -%}
              <li
                  class="menu-item menu-item-type-taxonomy menu-item-object-product_cat menu-item-has-children {% if childlink.current %} current-menu-item active {% endif %} {% if childlink.child_active %} current-menu-ancestor active {% endif %}">
                  <a href="{{ childlink.url }}">{{ childlink.title }}
                  </a>
                  {%- if childlink.links != blank -%}
                  <ul class="sub-menu">
                    {%- for childlinkLV2 in childlink.links -%}
                    <li
                      class="menu-item menu-item-type-post_type menu-item-object-product {% if childlinkLV2.current %} current-menu-item active {% endif %}"><a
                        href="{{ childlinkLV2.url }}">{{ childlinkLV2.title }}</a>
                    </li>
                    {%- endfor -%}
                  </ul>
                  {%- endif -%}
                </li>
              {%- endfor -%}
              </ul>
              </div>
               {%- endif -%}
            </li>
            {%- endfor -%}
        </ul>
      </div>
    </div>
  </nav>
</header>


{% schema %}
{
  "name": "New Header",
  "class": "section-header",
  "settings": [
    {
      "type": "image_picker",
      "id": "logo",
      "label": "t:sections.header.settings.logo.label"
    },
    {
      "type": "link_list",
      "id": "menu_top",
      "default": "main-menu",
      "label": "Menu Top"
    },
    {
      "type": "link_list",
      "id": "menu",
      "default": "main-menu",
      "label": "t:sections.header.settings.menu.label"
    },
    {
      "type": "link_list",
      "id": "menu_right",
      "default": "main-menu",
      "label": "Menu Right"
    },
    {
      "type": "select",
      "id": "menu_type_desktop",
      "options": [
        {
          "value": "dropdown",
          "label": "t:sections.header.settings.menu_type_desktop.options__1.label"
        },
        {
          "value": "mega",
          "label": "t:sections.header.settings.menu_type_desktop.options__2.label"
        }
      ],
      "default": "dropdown",
      "label": "t:sections.header.settings.menu_type_desktop.label",
      "info": "t:sections.header.settings.menu_type_desktop.info"
    },
    {
      "type": "checkbox",
      "id": "show_line_separator",
      "default": true,
      "label": "t:sections.header.settings.show_line_separator.label"
    },
    {
      "type": "checkbox",
      "id": "enable_sticky_header",
      "default": true,
      "label": "t:sections.header.settings.enable_sticky_header.label",
      "info": "t:sections.header.settings.enable_sticky_header.info"
    }
  ]
}
{% endschema %}